generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Skill {
  id        String   @id
  nameEn    String
  nameFr    String
  level     Int
  category  String
  createdAt DateTime @default(now())
  updatedAt DateTime
}
